{
  "name": "nestjs-utils",
  "version": "1.0.23",
  "description": "Nestjs Utils Package",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "predeploy": "npm version patch",
    "deploy": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/renanz/nestjs-utils.git"
  },
  "author": "Renan Zelaya",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/renanz/nestjs-utils/issues"
  },
  "files": [
    "lib/**/*"
  ],
  "homepage": "https://github.com/renanz/nestjs-utils#readme",
  "devDependencies": {
    "@nestjs/testing": "^7.5.5",
    "@types/jest": "^26.0.16",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@nestjs/common": "^7.5.5",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "^7.5.5",
    "@nestjs/graphql": "^7.9.1",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/terminus": "^7.0.1",
    "@nestjs/typeorm": "^7.1.4",
    "@tfarras/nestjs-firebase-admin": "^1.0.1",
    "@tfarras/nestjs-firebase-auth": "^1.0.0",
    "@types/passport-jwt": "^3.0.3",
    "aws-sdk": "^2.804.0",
    "joi": "^17.3.0",
    "passport-jwt": "^4.0.0",
    "rxjs": "^6.6.3",
    "typeorm": "0.2.28"
  }
}
